<?xml version="1.0"?>
<rolemap>
  <roles>
    <role name="Finanzas"/>
    <role name="Implant"/>
  </roles>
  <permissions>
    <!-- -*- extra stuff goes here -*- -->
    <permission name="viaticos.viajes: Crear comprobacion"
                acquire="True">
      <role name="Manager"/>
      <role name="Site Administrator"/>      
      <role name="Finanzas"/>
    </permission>
    
    <permission name="viaticos.viajes: Hacer notas comprobacion"
                acquire="True">
      <role name="Manager"/>
      <role name="Site Administrator"/>      
      <role name="Owner"/>
    </permission>

    <permission name="viaticos.viajes: Implant escribe"
                acquire="True">
      <role name="Manager"/>
      <role name="Site Administrator"/>      
      <role name="Implant"/>
    </permission>
    

  </permissions>
</rolemap>
